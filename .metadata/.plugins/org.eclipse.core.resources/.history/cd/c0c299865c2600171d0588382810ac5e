package goLA.model;

import java.util.HashMap;
import java.util.List;

public class TrajectoriesImpl implements Trajectories{
	
	public class Coord<X,R> {
	    private X l;
	    private R r;
	    public Coord(X l, R r){
	        this.l = l;
	        this.r = r;
	    }
	    public X getL(){ return l; }
	    public R getR(){ return r; }
	    public void setL(X l){ this.l = l; }
	    public void setR(R r){ this.r = r; }
	}
	

	private HashMap<Integer, List<Coord<Integer, Integer>>> t_map;
}
